- var home = match.home, away = match.away;
#main
	#score_others
		h2 Node.JS Championship
		ul
	#scoreboard
		span#team1.name= home.name
		span#team2.name= away.name
		span.score
			span#score1= home.score
			| &nbsp;x
			span#score2= away.score
	.content
		#content-left.module
			h3 Rooster
			#container-left.container
		#content-right.module
			h3 Rooster
			#container-right.container
		#content-center
			h3 Narration
			.container
				ul#narration
					- var plays = match.plays.concat(); plays.reverse();
					!= partial('partials/narration', { collection: plays, as: 'play', teams: teams })
	#timeline
		.teams
			#tTeam1= home.acronym
			hr
			#tTeam2= away.acronym
		.container
			hr#tLine
			table
				thead
					tr
						- for( var i = 1; i < 51; i++ )
							- if( i < 10 )
								- i = '0' + i;
							th #{i}

			#line1
			#line2
		br(clear='all')

ul#notify
